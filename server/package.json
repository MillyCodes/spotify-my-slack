{
  "name": "spotify-my-slack-server",
  "private": true,
  "scripts": {
    "build": "tsc",
    "heroku-postbuild": "sequelize db:migrate",
    "lint": "npm-run-all lint:*",
    "lint:eslint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "makemigration": "tsc && sequelize migration:generate --name",
    "sequelize": "tsc && sequelize",
    "shell": "node --interactive --eval \"require('dotenv').config()\"",
    "start": "node build/index.js",
    "watch": "tsc-watch --onSuccess \"npm start\" --preserveWatchOutput"
  },
  "dependencies": {
    "@slack/client": "^4.11.0",
    "async": "^2.6.1",
    "connect-redis": "^3.3.3",
    "ejs": "^2.5.6",
    "express": "^4.17.1",
    "express-session": "^1.15.6",
    "material-design-lite": "^1.3.0",
    "newrelic": "^4.6.0",
    "pg": "^7.4.3",
    "request": "^2.87.0",
    "sequelize": "^5.15.1",
    "spotify-web-api-node": "kauffecup/spotify-web-api-node#error-headers"
  },
  "devDependencies": {
    "dotenv": "^6.0.0",
    "eslint": "^6.7.2",
    "npm-run-all": "^4.1.5",
    "sequelize-cli": "^5.5.0",
    "tsc-watch": "^4.0.0",
    "typescript": "^3.7.3"
  },
  "eslintConfig": {
  "parserOptions": {
    "ecmaVersion": 2017
  },
    "env": {
      "es6": true
    }
  }
}
