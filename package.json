{
  "name": "spotify-my-slack",
  "version": "0.0.2",
  "description": "Show what's playing on Spotify as your Slack status",
  "engines": {
    "node": "12.13.1"
  },
  "scripts": {
    "build": "scripts/build.sh",
    "start": "cd server && npm start",
    "test": "scripts/test.sh",
    "test:prettier": "prettier --check \"{client/src,scripts,server/src}/**/*.{js,jsx,ts,tsx}\""
  },
  "devDependencies": {
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/micthiesen/spotify-my-slack"
  },
  "cacheDirectories": [
    "client/node_modules",
    "server/node_modules"
  ],
  "keywords": [
    "express",
    "heroku",
    "node",
    "slack",
    "spotify"
  ],
  "license": "GPL-3.0-only"
}
